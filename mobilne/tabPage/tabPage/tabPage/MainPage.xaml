<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:tabPage" 
            x:Class="tabPage.MainPage"
            BarBackgroundColor="Red"
            BarTextColor="Black">

    <ContentPage Title="Start" IconImageSource="male.png">
        <StackLayout HorizontalOptions="Center"
                     VerticalOptions="Center">
            <Label Text="Strona startowa"/>
        </StackLayout>
    </ContentPage>

    <ContentPage Title="Bio">
        <StackLayout HorizontalOptions="Center"
                     VerticalOptions="Center">
            <Label Text="Informacje o mnie"/>
        </StackLayout>
    </ContentPage>

    <local:ContactPage/>
</TabbedPage>

<Window x:Class="wpfJump.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpfJump"
        mc:Ignorable="d"
        Title="WPF Jump" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="40">
            <StackPanel Orientation="Horizontal">
                <Label Content="Podaj s:"/>
                <TextBox Height="15" Width="50" Margin="0, 7" VerticalAlignment="Top" x:Name="sTextBox"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="Podaj n:"/>
                <TextBox Height="15" Width="50" Margin="0, 7" VerticalAlignment="Top" x:Name="nTextBox"/>
            </StackPanel>
            <Button Content="Wykonaj" Width="200" HorizontalAlignment="Left" x:Name="execButton" Click="execButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
